<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="Demo Cart App Regression Test Suite" thread-count="3"
	parallel="tests" verbose="3">


	
	<listeners>
		<listener
			class-name="com.qa.opencart.listeners.ExtentReportListener" />
		<listener
			class-name="com.qa.opencart.listeners.TestAllureListener" />
	</listeners>
	


	<test name="Login Page Test Firefox_57">
		<parameter name ="browser" value ="firefox" />
		<parameter name ="version" value ="57" />
		<classes>
			<class name="com.qa.opencart.tests.LoginPageTest" />
		</classes>
	</test>
	
	<test name="Login Page Test Firefox_83">
		<parameter name ="browser" value ="firefox" />
		<parameter name ="version" value ="83" />
		<classes>
			<class name="com.qa.opencart.tests.LoginPageTest" />
		</classes>
	</test>

	<test name="Accounts Page Test Firefox_80 ">
	<parameter name ="browser" value ="firefox" />
		<parameter name ="version" value ="80" />
		<classes>
			<class name="com.qa.opencart.tests.AccountsPageTest" />
		</classes>
	</test>


	
	<test name="Register Page Test Firefox_85">
		<parameter name ="browser" value ="firefox" />
		<parameter name ="version" value ="85" />
		<classes>
			<class name="com.qa.opencart.tests.RegisterPageTest" />
		</classes>
	</test>


	<test name="ShoppingCart Page Test Firefox_85">
	<parameter name ="browser" value ="firefox" />
		<parameter name ="version" value ="85" />
		<classes>
			<class name="com.qa.opencart.tests.ProductPageInfoTest" />
		</classes>
	</test>
	
	<test name="ProductInfo Page Test Firefox_83">
	<parameter name ="browser" value ="firefox" />
		<parameter name ="version" value ="83" />
		<classes>
			<class name="com.qa.opencart.tests.AccountsPageTest" />
		</classes>
	</test>


</suite>